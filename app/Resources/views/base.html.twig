<!DOCTYPE html>

<!--[if IE 8]> <html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!--> <html lang="en" class="no-js"> <!--<![endif]-->

<head>
    <meta charset="utf-8">
    <title>ভার্চুয়াল রেকর্ড রুম</title>
    <link rel="icon" type="image/ico" href="http://dcms.e-service.gov.bd/assets/templates/default/images/ico.ico" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport">
    <meta content="" name="description">
    <meta content="" name="author">
    <script type="text/javascript">window.onloadQueue = window.onloadQueue || [];</script>
    <link rel="shortcut icon" href="favicon.ico">
    <!-- <link href="assets/global/css/components.css" rel="stylesheet">
     <link href="assets/frontend/layout/css/style.css" rel="stylesheet">
     <link href="assets/frontend/layout/css/style-responsive.css" rel="stylesheet">
     <link href="assets/frontend/layout/css/themes/red.css" rel="stylesheet" id="style-color">
     <link href="assets/frontend/layout/css/custom.css" rel="stylesheet">-->
    <!-- Fonts START -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=all" rel="stylesheet" type="text/css">
    <!-- Fonts END -->
    {% block stylesheets %}
        {% stylesheets filter='cssrewrite'
        'assets/global/plugins/font-awesome/css/font-awesome.min.css'
        'assets/global/plugins/simple-line-icons/simple-line-icons.min.css'
        'assets/global/plugins/bootstrap/css/bootstrap.min.css'
        'assets/global/plugins/fancybox/source/jquery.fancybox.css'
        'assets/global/plugins/uniform/css/uniform.default.css'
        'assets/global/plugins/slider-layer-slider/css/layerslider.css'
        'assets/global/plugins/carousel-owl-carousel/owl-carousel/owl.carousel.css'

        'assets/global/plugins/intl-tel-input-master/build/css/intlTelInput.css'

        'assets/global/plugins/select2/select2.css'
        'assets/global/plugins/bootstrap-datepicker/css/datepicker3.css'

        'assets/global/plugins/morris/morris.css'
        'assets/admin/pages/css/tasks.css'
        'assets/global/plugins/bootstrap-toastr/toastr.min.css'

        'assets/global/plugins/bootstrap-fileinput/bootstrap-fileinput.css'

        'assets/global/css/components-rounded.css'
        'assets/global/css/plugins.css'
        'assets/admin/layout3/css/layout.css'
        'assets/admin/layout3/css/themes/default.css'
        'assets/admin/layout3/css/custom.css'
        %}
        <link rel="stylesheet" href="{{ asset_url }}"/>
        {% endstylesheets %}
    {% endblock  %}

</head>
<body>
<div class="container">
{{ include(':Frontend:recordCheck.html.twig')}}
{{ include('header.html.twig')}}

{% block body %}{% endblock %}
{% block javascriptBlock %}

    {% javascripts

    'assets/global/plugins/excanvas.min.js'
    'assets/global/plugins/respond.min.js'

    'assets/global/plugins/jquery.min.js'
    'assets/global/plugins/jquery-migrate.min.js'
    'assets/global/plugins/jquery-ui/jquery-ui-1.10.3.custom.min.js'
    'assets/global/plugins/bootstrap/js/bootstrap.min.js'
    'assets/global/plugins/bootstrap-hover-dropdown/bootstrap-hover-dropdown.min.js'
    'assets/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js'
    'assets/global/plugins/jquery.blockui.min.js'
    'assets/global/plugins/jquery.cokie.min.js'
    'assets/global/plugins/uniform/jquery.uniform.min.js'
    'assets/global/plugins/slider-layer-slider/js/greensock.js'
    'assets/global/plugins/slider-layer-slider/js/layerslider.transitions.js'
    'assets/global/plugins/slider-layer-slider/js/layerslider.kreaturamedia.jquery.js'
    'assets/frontend/pages/scripts/layerslider-init.js'
    'assets/global/plugins/select2/select2.min.js'

    'assets/global/plugins/intl-tel-input-master/build/js/intlTelInput.js'

    'assets/global/plugins/bootstrap-fileinput/bootstrap-fileinput.js'
    'assets/global/plugins/bootstrap-toastr/toastr.min.js'

    'assets/global/plugins/jquery-validation/js/jquery.validate.min.js'
    'assets/global/plugins/jquery-validation/js/additional-methods.min.js'
    'assets/global/plugins/pace/pace.min.js'
    'assets/global/plugins/morris/morris.min.js'
    'assets/global/plugins/morris/raphael-min.js'
    'assets/global/plugins/jquery.sparkline.min.js'
    'assets/global/scripts/metronic.js'
    'assets/admin/layout3/scripts/layout.js'
    'assets/elrs-assets/js/index-app-bd.min.js'

    '@AppBundle/Resources/public/js/recordCheck.js'

    '@FOSJsRoutingBundle/Resources/public/js/router.js'
    'js/fos_js_routes.js'
    %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock javascriptBlock %}

<script type="text/javascript">
    jQuery(document).ready(function () {
        {% block documentReady %}
        {% if app.request.hasPreviousSession %}
            {% for type, messages in app.session.flashbag.all() %}
                {% for message in messages %}

                    toastr.{{ type }}('{{ message }}','', {timeOut: 10000, hideMethod: "hide", closeButton: true});

                {% endfor %}
            {% endfor %}
        {% endif %}
        Layout.init(); // init layout
        Metronic.init(); // init metronic core componets
        LayersliderInit.initLayerSlider();
        telInput.intlTelInput({
            utilsScript: "{{ asset('assets/global/plugins/intl-tel-input-master/build/js/utils.js') }}",
            nationalMode: true,
            preferredCountries: ['bd', 'in']
        });

        {% endblock %}
    });
</script>
{{ include('footer.html.twig')}}
</div>
</body>

</html>
