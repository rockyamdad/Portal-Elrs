
<div class="row table-responsive">
    <div class="col-lg-12">
        <table class="table" id="search_khatian">
            <thead>
            <tr>
                <th>
                    ক্রম নং
                </th>

                <th>
                    উপজেলা /থানা
                </th>
                <th>
                    মৌজা
                </th>
                <th>
                    জে .এল. নং
                </th>
                <th>
                    খতিয়ান/পর্চা  নং
                </th>
                <th>
                    সার্ভে
                </th>

                <th>
                    খতিয়ানা দেখুন
                </th>

            </tr>
            </thead>
            <tbody>
            {% if porcha and porcha %}
            {% set i =1 %}
            {% for  p in porcha %}
                <tr>
                    <td>{{ i }}</td>
                    <td>{{ p.upozila }}</td>
                    <td>{{ p.mouza }}</td>
                    <td>{{ p.jlNumber }}</td>
                    <td>{{ p.khatianNo }}</td>
                    <td>{{ p.survey }}</td>
                    <td><a href="{{ path('khatian_show',{'id':p.id}) }}" target="_blank">  <i class="fa fa-file-o"></i> </a></td>

                </tr>
                {% set i = i+1 %}
            {% endfor %}
            {% else %}
                <tr>
                    <td colspan="6" style="text-align: center">
                        <h4 class="error">আপনার রেকর্ড টি পাওয়া যাই নি </h4>
                    </td>
                </tr>
            {% endif %}
            </tbody>
        </table>
    </div>
</div>
